{
  "name": "infographics_cdk",
  "version": "1.0.0",
  "description": "Editorial Infographics infrastructure stack using S3 and Transfer Family",
  "main": "src/index.ts",
  "scripts": {
    "cdk": "cdk",
    "cdk:deploy": "npm run build && npm run cdk -- deploy '*' --require-approval never",
    "cdk:synth": "npm run cdk -- synth '*'",
    "cdk:destroy": "npm run cdk -- destroy --all",
    "lint": "eslint .",
    "test": "jest",
    "build": "rm -rf build/ && tsc functions/*/*.ts --outDir build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.345.0",
    "aws-cdk": "^2.82.0",
    "aws-cdk-lib": "^2.82.0",
    "aws-sdk": "^2.1390.0",
    "constructs": "^10.2.43",
    "dotenv": "^16.1.4"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1"
  }
}
